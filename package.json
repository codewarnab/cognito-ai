{
  "name": "chrome-ai",
  "displayName": "Chrome ai",
  "version": "0.0.1",
  "description": "super powerful and private ",
  "author": "dev@codewarnab.in",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "dexie": "^4.2.1",
    "minisearch": "^7.2.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/jsdom": "^27.0.0",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^19.0.2",
    "jsdom": "^27.0.0",
    "prettier": "3.2.4",
    "typescript": "5.3.3",
    "vitest": "^3.2.4"
  },
  "manifest": {
    "permissions": [
      "storage",
      "offscreen",
      "unlimitedStorage",
      "scripting",
      "alarms",
      "tabs",
      "sidePanel"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "offscreen.html"
        ],
        "matches": [
          "<all_urls>"
        ]
      },
      {
        "resources": [
          "models/1.0.0/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    }
  }
}