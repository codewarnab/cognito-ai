{
  "name": "chrome-ai",
  "displayName": "Chrome ai",
  "version": "0.0.1",
  "description": "super powerful and private ",
  "author": "dev@codewarnab.in",
  "scripts": {
    "dev": "plasmo dev",
    "dev:setup": "node scripts/copy-offscreen.js",
    "build": "plasmo build && node scripts/copy-offscreen.js",
    "package": "plasmo package",
    "postinstall": "node scripts/patch-vfile.js && node scripts/patch-google-sdk.js"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/react": "^2.0.75",
    "@copilotkit/react-core": "^1.10.6",
    "@copilotkit/react-ui": "^1.10.6",
    "@copilotkit/runtime-client-gql": "^1.10.6",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@plasmohq/messaging": "^0.7.2",
    "@plasmohq/storage": "^1.15.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "ai": "^5.0.75",
    "cmdk": "^1.1.1",
    "dexie": "^4.2.1",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.546.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^10.1.0",
    "rehype-sanitize": "^6.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/jsdom": "^27.0.0",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "happy-dom": "^19.0.2",
    "jsdom": "^27.0.0",
    "patch-package": "^8.0.1",
    "postcss": "^8.1.0",
    "prettier": "3.2.4",
    "tailwindcss": "^4.1.14",
    "typescript": "5.3.3"
  },
  "manifest": {
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    },
    "permissions": [
      "storage",
      "unlimitedStorage",
      "tabs",
      "tabGroups",
      "sidePanel",
      "alarms",
      "scripting",
      "activeTab",
      "webNavigation",
      "identity",
      "notifications",
      "offscreen",
      "cookies",
      "history",
      "search",
      "bookmarks"
    ],
    "optional_permissions": [
      "tabCapture",
      "desktopCapture"
    ],
    "host_permissions": [
      "https://backend-4khk5owpk-arnab-mondals-projects.vercel.app/*",
      "http://localhost:3000/*",
      "https://*/*",
      "http://*/*",
      "https://api.notion.com/*",
      "https://mcp.notion.com/*",
      "https://www.notion.so/*"
    ],
    "commands": {
      "_execute_side_panel": {
        "suggested_key": {
          "default": "Ctrl+Shift+H",
          "mac": "Command+Shift+H"
        },
        "description": "Open AI Chat side panel"
      }
    },
    "side_panel": {
      "default_path": "sidepanel.html"
    }
  }
}